	sh:nodeKind sh:IRI ;
	sh:minCount	"1"^^xsd:nonNegativeInteger ;
	sh:maxCount	"1"^^xsd:nonNegativeInteger ;
	sh:path     :address ;
	sh:class  	:Address  .
	
