@prefix :      <https://www.w3id.org/def/reo#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix val: <http://example.com/validation_shape#> .

val:RealEstate a sh:NodeShape ;
	sh:nodeKind sh:IRI ;
	sh:targetClass :RealEstate ;
	sh:or (
		[
			sh:nodeKind sh:IRI ;
			sh:minCount	"1"^^xsd:nonNegativeInteger ;
			sh:maxCount	"1"^^xsd:nonNegativeInteger ;
			sh:path     :apartmentType ;
			sh:class  	:ApartmentType  ;	
		]
		[
			sh:nodeKind sh:IRI ;
			sh:minCount	"1"^^xsd:nonNegativeInteger ;
			sh:maxCount	"1"^^xsd:nonNegativeInteger ;
			sh:path     :houseType ;
			sh:class  	:HouseType  ;	
		]	
	) ;
	sh:or (
		[
			sh:nodeKind sh:Literal ;
			sh:minCount	"1"^^xsd:nonNegativeInteger ;
			sh:maxCount	"1"^^xsd:nonNegativeInteger ;
			sh:path     :netRent ;
			sh:datatype xsd:float ;
		]
		[
			sh:nodeKind sh:Literal ;
			sh:minCount	"1"^^xsd:nonNegativeInteger ;
			sh:maxCount	"1"^^xsd:nonNegativeInteger ;
			sh:path     :purchasePrice ;
			sh:datatype xsd:float ;
		]
	) ;
	sh:property val:address, val:contact, val:energyPass, val:marketingType, val:realEstateType, val:usageType, val:tenantGuidelines, 
				val:hasBalcony, val:hasCellar, val:hasElevator, val:hasFittedKitchen, val:hasGarden, val:hasGuestToilet, val:hasWashingMachineConnection,
				val:bail, val:carspaceCost, val:additionalCost, val:heatingCost, val:minIncome, val:modernizationYear, val:constructionYear,
				val:floor, val:floorCount, val:livingSpace, val:maxHouseholdPersons, val:title  .	
	
