@prefix : <https://www.w3id.org/def/reo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix reo: <https://www.w3id.org/def/reo#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@base <https://www.w3id.org/def/reo> .

<https://www.w3id.org/def/reo> rdf:type owl:Ontology ;
                                dc:creator "Tim Fock" ;
                                terms:licence <http://purl.org/NET/rdflicense/cc-by4.0> ;
                                terms:title "Real Estate Ontology" ;
                                rdfs:comment "This ontology describes real estate using a controlled vocabulary based on a dataset from a prop-tech company. It is aimed at the german market and models some concepts only valid in Germany (e.g., energy pass)." ;
                                owl:versionInfo "0.0.1" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/licence
terms:licence rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
terms:title rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://www.w3id.org/def/reo#address
reo:address rdf:type owl:ObjectProperty ;
            rdfs:label "Adresse"@de .


###  https://www.w3id.org/def/reo#apartmentType
reo:apartmentType rdf:type owl:ObjectProperty ;
                  rdfs:label "Apartmenttyp"@de .


###  https://www.w3id.org/def/reo#cat
reo:cat rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf reo:tenantGuidelines ;
        rdfs:label "Katze"@de .


###  https://www.w3id.org/def/reo#commercialUse
reo:commercialUse rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf reo:tenantGuidelines ;
                  rdfs:label "kommerzielle Nutzung"@de .


###  https://www.w3id.org/def/reo#condition
reo:condition rdf:type owl:ObjectProperty ;
              rdfs:label "Zustand"@de .


###  https://www.w3id.org/def/reo#dog
reo:dog rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf reo:tenantGuidelines ;
        rdfs:label "Hund"@de .


###  https://www.w3id.org/def/reo#energyCarrier
reo:energyCarrier rdf:type owl:ObjectProperty ;
                  rdfs:label "Energieträger"@de .


###  https://www.w3id.org/def/reo#energyEfficiencyClass
reo:energyEfficiencyClass rdf:type owl:ObjectProperty ;
                          rdfs:label "Energieeffizienzklasse"@de .


###  https://www.w3id.org/def/reo#energyPass
reo:energyPass rdf:type owl:ObjectProperty ;
               rdfs:label "Energieausweis"@de .


###  https://www.w3id.org/def/reo#energypassType
reo:energypassType rdf:type owl:ObjectProperty ;
                   rdfs:label "Energieausweistyp"@de .


###  https://www.w3id.org/def/reo#equipmentCondition
reo:equipmentCondition rdf:type owl:ObjectProperty ;
                       rdfs:label "Zustand der Inneneinrichtung"@de .


###  https://www.w3id.org/def/reo#heatingType
reo:heatingType rdf:type owl:ObjectProperty ;
                rdfs:label "Heizungstyp"@de .


###  https://www.w3id.org/def/reo#livingCommunity
reo:livingCommunity rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf reo:tenantGuidelines ;
                    rdfs:label "Wohngemeinschaft"@de .


###  https://www.w3id.org/def/reo#marketingType
reo:marketingType rdf:type owl:ObjectProperty ;
                  rdfs:label "Vermarktungsart"@de .


###  https://www.w3id.org/def/reo#otherPet
reo:otherPet rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf reo:tenantGuidelines ;
             rdfs:label "anderes Haustier"@de .


###  https://www.w3id.org/def/reo#realEstateType
reo:realEstateType rdf:type owl:ObjectProperty ;
                   rdfs:label "Immobilientyp"@de .


###  https://www.w3id.org/def/reo#smoking
reo:smoking rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf reo:tenantGuidelines ;
            rdfs:label "Rauchen"@de .


###  https://www.w3id.org/def/reo#tenantGuidelines
reo:tenantGuidelines rdf:type owl:ObjectProperty ;
                     rdfs:label "Richtlinien für Mieter" .


###  https://www.w3id.org/def/reo#usageType
reo:usageType rdf:type owl:ObjectProperty ;
              rdfs:label "Nutzungsart"@de .


#################################################################
#    Data properties
#################################################################

###  https://www.w3id.org/def/reo#additionalCost
reo:additionalCost rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf reo:cost ;
                   rdfs:label "Nebenkosten"@de .


###  https://www.w3id.org/def/reo#bail
reo:bail rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf reo:cost ;
         rdfs:label "Kaution"@de .


###  https://www.w3id.org/def/reo#carspaceCost
reo:carspaceCost rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf reo:cost ;
                 rdfs:label "Kosten für Stellplatz"@de .


###  https://www.w3id.org/def/reo#city
reo:city rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:range xsd:string ;
         rdfs:label "Stadt"@de .


###  https://www.w3id.org/def/reo#constructionYear
reo:constructionYear rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf reo:year ;
                     rdfs:range xsd:int ;
                     rdfs:label "Baujahr"@de .


###  https://www.w3id.org/def/reo#cost
reo:cost rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:label "Kosten"@de .


###  https://www.w3id.org/def/reo#countryCode
reo:countryCode rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:range xsd:string ;
                rdfs:label "Ländercode"@de .


###  https://www.w3id.org/def/reo#floor
reo:floor rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:int ;
          rdfs:label "Stockwerk"@de .


###  https://www.w3id.org/def/reo#floorCount
reo:floorCount rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:range xsd:int ;
               rdfs:label "Anzahl der Stockwerke"@de .


###  https://www.w3id.org/def/reo#hasCellar
reo:hasCellar rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:boolean ;
              rdfs:label "hat Keller"@de .


###  https://www.w3id.org/def/reo#hasFittedKitchen
reo:hasFittedKitchen rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:boolean ;
                     rdfs:label "hat Einbauküche"@de .


###  https://www.w3id.org/def/reo#hasGarden
reo:hasGarden rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:range xsd:boolean ;
              rdfs:label "hat Garten"@de .


###  https://www.w3id.org/def/reo#hasWashingMachineConnection
reo:hasWashingMachineConnection rdf:type owl:DatatypeProperty ;
                                rdfs:range xsd:boolean ;
                                rdfs:label "hat Waschmaschinenanschluss"@de .


###  https://www.w3id.org/def/reo#heatingCost
reo:heatingCost rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf reo:cost ;
                rdfs:label "Heizkosten"@de .


###  https://www.w3id.org/def/reo#houseNumber
reo:houseNumber rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:range xsd:int ;
                rdfs:label "Hausnummer"@de .


###  https://www.w3id.org/def/reo#livingSpace
reo:livingSpace rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:range xsd:double ;
                rdfs:label "Wohnfläche"@de .


###  https://www.w3id.org/def/reo#maxHouseholdPersons
reo:maxHouseholdPersons rdf:type owl:DatatypeProperty ;
                        rdfs:range xsd:int ;
                        rdfs:label "maximale Anzahl der Personen im Haushalt"@de .


###  https://www.w3id.org/def/reo#modernizationYear
reo:modernizationYear rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf reo:year ;
                      rdfs:range xsd:int ;
                      rdfs:label "Jahr der Modernisierung"@de .


###  https://www.w3id.org/def/reo#netRent
reo:netRent rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf reo:cost ;
            rdfs:label "Nettokaltmiete"@de .


###  https://www.w3id.org/def/reo#numberOf
reo:numberOf rdf:type owl:DatatypeProperty ;
             rdfs:label "Anzahl"@de .


###  https://www.w3id.org/def/reo#numberOfBathrooms
reo:numberOfBathrooms rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf reo:numberOfRooms ;
                      rdfs:range xsd:int ;
                      rdfs:label "Anzahl der Badezimmer"@de .


###  https://www.w3id.org/def/reo#numberOfBedrooms
reo:numberOfBedrooms rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf reo:numberOfRooms ;
                     rdfs:range xsd:int ;
                     rdfs:label "Anzahl der Schlafzimmer"@de .


###  https://www.w3id.org/def/reo#numberOfCarSpaces
reo:numberOfCarSpaces rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf reo:numberOf ;
                      rdfs:range xsd:int ;
                      rdfs:label "Anzahl der Stellplätze"@de .


###  https://www.w3id.org/def/reo#numberOfRooms
reo:numberOfRooms rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf reo:numberOf ;
                  rdfs:range xsd:int ;
                  rdfs:label "Anzahl der Zimmer"@de .


###  https://www.w3id.org/def/reo#purchasePrice
reo:purchasePrice rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf reo:cost ;
                  rdfs:label "Kaufpreis"@de .


###  https://www.w3id.org/def/reo#street
reo:street rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range xsd:string ;
           rdfs:label "Straße"@de .


###  https://www.w3id.org/def/reo#title
reo:title rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:string ;
          rdfs:label "Titel"@de .


###  https://www.w3id.org/def/reo#year
reo:year rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:label "Jahr"@de .


###  https://www.w3id.org/def/reo#zipCode
reo:zipCode rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:range xsd:int ;
            rdfs:label "Postleitzahl"@de .


#################################################################
#    Classes
#################################################################

###  https://www.w3id.org/def/reo#AbstractFeature
reo:AbstractFeature rdf:type owl:Class ;
                    rdfs:subClassOf reo:Feature ;
                    owl:disjointWith reo:PhysicalFeature ;
                    rdfs:label "Abstrakte Eigenschaft"@de .


###  https://www.w3id.org/def/reo#Address
reo:Address rdf:type owl:Class ;
            rdfs:subClassOf reo:AbstractFeature ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty reo:city ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty reo:countryCode ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty reo:houseNumber ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:int
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty reo:street ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty reo:zipCode ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:int
                            ] ;
            rdfs:label "Adresse"@de .


###  https://www.w3id.org/def/reo#ApartmentType
reo:ApartmentType rdf:type owl:Class ;
                  rdfs:subClassOf reo:Type ;
                  rdfs:label "Wohnungstyp"@de .


###  https://www.w3id.org/def/reo#CarSpace
reo:CarSpace rdf:type owl:Class ;
             rdfs:subClassOf reo:PhysicalFeature ;
             rdfs:label "Stellplatz"@de .


###  https://www.w3id.org/def/reo#CaretakerContact
reo:CaretakerContact rdf:type owl:Class ;
                     rdfs:subClassOf reo:AbstractFeature ;
                     rdfs:label "Ansprechpartner"@de .


###  https://www.w3id.org/def/reo#Condition
reo:Condition rdf:type owl:Class ;
              rdfs:subClassOf reo:AbstractFeature ;
              rdfs:label "Zustand"@de .


###  https://www.w3id.org/def/reo#EnergyCarrier
reo:EnergyCarrier rdf:type owl:Class ;
                  rdfs:subClassOf reo:AbstractFeature ;
                  rdfs:label "Energieträger"@de .


###  https://www.w3id.org/def/reo#EnergyEfficiencyClass
reo:EnergyEfficiencyClass rdf:type owl:Class ;
                          rdfs:subClassOf reo:AbstractFeature ;
                          rdfs:label "Energieeffizienz"@de .


###  https://www.w3id.org/def/reo#EnergyPass
reo:EnergyPass rdf:type owl:Class ;
               rdfs:subClassOf reo:AbstractFeature ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:energyCarrier ;
                                 owl:someValuesFrom reo:EnergyCarrier
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:heatingType ;
                                 owl:someValuesFrom reo:HeatingType
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:energyEfficiencyClass ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass reo:EnergyEfficiencyClass
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:energypassType ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass reo:EnergyPassType
                               ] ;
               rdfs:label "Energieausweis"@de .


###  https://www.w3id.org/def/reo#EnergyPassType
reo:EnergyPassType rdf:type owl:Class ;
                   rdfs:subClassOf reo:AbstractFeature ;
                   rdfs:label "Energieausweistyp"@de .


###  https://www.w3id.org/def/reo#EquipmentCondition
reo:EquipmentCondition rdf:type owl:Class ;
                       rdfs:subClassOf reo:AbstractFeature ;
                       rdfs:label "Zustand der Inneneinrichtung"@de .


###  https://www.w3id.org/def/reo#Feature
reo:Feature rdf:type owl:Class ;
            rdfs:label "Eigenschaft"@de .


###  https://www.w3id.org/def/reo#HeatingType
reo:HeatingType rdf:type owl:Class ;
                rdfs:subClassOf reo:AbstractFeature ;
                rdfs:label "Heizungstyp"@de .


###  https://www.w3id.org/def/reo#MarketingType
reo:MarketingType rdf:type owl:Class ;
                  rdfs:subClassOf reo:Type ;
                  rdfs:label "Vermarktungsart"@de .


###  https://www.w3id.org/def/reo#PhysicalFeature
reo:PhysicalFeature rdf:type owl:Class ;
                    rdfs:subClassOf reo:Feature ;
                    rdfs:label "Physische Eigenschaft"@de .


###  https://www.w3id.org/def/reo#RealEstate
reo:RealEstate rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Class ;
                                 owl:unionOf ( [ rdf:type owl:Restriction ;
                                                 owl:onProperty reo:netRent ;
                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onDataRange xsd:double
                                               ]
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty reo:purchasePrice ;
                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onDataRange xsd:double
                                               ]
                                             )
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:tenantGuidelines ;
                                 owl:someValuesFrom reo:TenantGuidelines
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:address ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass reo:Address
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:apartmentType ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass reo:ApartmentType
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:marketingType ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass reo:MarketingType
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:realEstateType ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass reo:RealEstateType
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:usageType ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass reo:UsageType
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:energyPass ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass reo:EnergyPass
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:livingSpace ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:double
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:title ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:additionalCost ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:double
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:bail ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:double
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:carspaceCost ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:double
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:constructionYear ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:int
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:floor ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:int
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:floorCount ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:int
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:hasCellar ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:boolean
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:hasFittedKitchen ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:boolean
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:hasGarden ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:boolean
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:hasWashingMachineConnection ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:boolean
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:heatingCost ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:double
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:maxHouseholdPersons ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:int
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty reo:modernizationYear ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:int
                               ] ;
               rdfs:label "Immobilie"@de .


###  https://www.w3id.org/def/reo#RealEstateType
reo:RealEstateType rdf:type owl:Class ;
                   rdfs:subClassOf reo:Type ;
                   rdfs:label "Immobilientyp"@de .


###  https://www.w3id.org/def/reo#TenantGuidelines
reo:TenantGuidelines rdf:type owl:Class ;
                     rdfs:subClassOf reo:AbstractFeature ;
                     rdfs:label "Richtlinien für Mieter"@de .


###  https://www.w3id.org/def/reo#Type
reo:Type rdf:type owl:Class ;
         rdfs:label "Fixe Werte"@de .


###  https://www.w3id.org/def/reo#UsageType
reo:UsageType rdf:type owl:Class ;
              rdfs:subClassOf reo:Type ;
              rdfs:label "Nutzungsart"@de .


#################################################################
#    Individuals
#################################################################

###  https://www.w3id.org/def/reo#A
reo:A rdf:type owl:NamedIndividual ,
               reo:EnergyEfficiencyClass ;
      rdfs:label "A"@de .


###  https://www.w3id.org/def/reo#AGREEMENT
reo:AGREEMENT rdf:type owl:NamedIndividual ,
                       reo:TenantGuidelines ;
              rdfs:label "nach Vereinbarung"@de .


###  https://www.w3id.org/def/reo#ALLOWED
reo:ALLOWED rdf:type owl:NamedIndividual ,
                     reo:TenantGuidelines ;
            rdfs:label "erlaubt"@de .


###  https://www.w3id.org/def/reo#ALTERNATIVE
reo:ALTERNATIVE rdf:type owl:NamedIndividual ,
                         reo:EnergyCarrier ;
                rdfs:label "Alternativ"@de .


###  https://www.w3id.org/def/reo#APARTMENT
reo:APARTMENT rdf:type owl:NamedIndividual ,
                       reo:ApartmentType ;
              rdfs:label "Wohnung"@de .


###  https://www.w3id.org/def/reo#ASAGREED
reo:ASAGREED rdf:type owl:NamedIndividual ,
                      reo:Condition ;
             rdfs:label "nach Vereinbarung"@de .


###  https://www.w3id.org/def/reo#A_PLUS
reo:A_PLUS rdf:type owl:NamedIndividual ,
                    reo:EnergyEfficiencyClass ;
           rdfs:label "A+"@de .


###  https://www.w3id.org/def/reo#B
reo:B rdf:type owl:NamedIndividual ,
               reo:EnergyEfficiencyClass ;
      rdfs:label "B"@de .


###  https://www.w3id.org/def/reo#C
reo:C rdf:type owl:NamedIndividual ,
               reo:EnergyEfficiencyClass ;
      rdfs:label "C"@de .


###  https://www.w3id.org/def/reo#CARBON
reo:CARBON rdf:type owl:NamedIndividual ,
